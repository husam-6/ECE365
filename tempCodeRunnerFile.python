def isPalindrome(x):
        if(x<0):
            return False
       
        x = str(x)
        tmp = [c for c in x]
       
        left = 0 
        right = len(tmp)-1
        
        while(left != right):
            if(tmp[left] != tmp[right]):
               return False
            left+=1
            right-=1
       
        return True

print(isPalindrome(10))